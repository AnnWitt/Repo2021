            private static void przykladBrak() {
                for (int i=0; i<10;i++) {
                    if (i==3) {break;} --> konczy
                        System.out.println(i);
                }
            }

            private static void przykladContinue() {
                for (int i=0; i<10;i++) {
                    if (i==8) {continue;} //wyswietli bez 8
                    System.out.println(i);
                }
            }

